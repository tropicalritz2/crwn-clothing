ucturedSelector({\n  currentUser: selectCurrentUser\n});\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/Users/ritterj/Development/crwn-clothing/src/serviceWorkerRegistration.js",[],["171","172"],"/Users/ritterj/Development/crwn-clothing/src/reportWebVitals.js",[],"/Users/ritterj/Development/crwn-clothing/src/pages/homepage/homepage.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/directory/directory.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/menu-item/menu-item.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/pages/shop/shop.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/collection-preview/collection-preview.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/collection-item/collection-item.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/header/header.component.jsx",["173","174"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { auth } from '../../firebase/firebase.utils';\nimport { connect } from 'react-redux';\nimport CartIcon from '../cart-icon/cart-icon.component';\nimport CartDropdown from '../cart-dropdown/cart-dropdown.component';\nimport { createStructuredSelector } from 'reselect';\nimport { selectCartHidden, selectCartItemsCount } from '../../redux/cart/cart.selectors';\nimport { select, selectCurrentUser } from '../../redux/user/user.selectors';\n\nimport { ReactComponent as Logo } from '../../assets/crown.svg'\n\nimport './header.styles.scss';\n\nconst Header = ({ currentUser, hidden }) => (\n    <div className='header'>\n        <Link className='logo-container' to='/'>\n           <Logo className='logo' /> \n        </Link>\n        <div className='options'>\n            <Link className='option' to='/shop'>\n                SHOP\n            </Link>\n            <Link className='option' to='/shop'>\n                CONTACT\n            </Link>\n            {currentUser ? (\n                <div className='option' onClick={() => auth.signOut()}>SIGN OUT</div>\n                    ) : (\n                <Link className='option' to=\"/signin\">SIGN IN</Link>\n            )}\n            <CartIcon />\n        </div>\n        {hidden ? null : <CartDropdown />}\n    </div>\n);\n\nconst mapStateToProps = createStructuredSelector({\n    currentUser: selectCurrentUser,\n    hidden: selectCartHidden\n});\n\nexport default connect(mapStateToProps)(Header);","/Users/ritterj/Development/crwn-clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/form-input/form-input.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/pages/sign-in-and-sign-up/sign-in-and-sign-up.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/firebase/firebase.utils.js",[],"/Users/ritterj/Development/crwn-clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/redux/store.js",["175"],"import { createStore, applyMiddleware } from 'redux';\nimport { persistStore } from 'redux-persist';\nimport logger from 'redux-logger';\n\nimport rootReducer from './root-reducer';\n\nconst middlewares = [logger];\n\nexport const store = createStore(rootReducer, applyMiddleware(...middlewares));\n\nexport const persistor = persistStore(store);\n\nexport default { store, persistor };","/Users/ritterj/Development/crwn-clothing/src/redux/root-reducer.js",[],"/Users/ritterj/Development/crwn-clothing/src/redux/user/user.reducer.jsx",[],"/Users/ritterj/Development/crwn-clothing/src/redux/user/user.actions.js",[],"/Users/ritterj/Development/crwn-clothing/src/redux/user/user.types.js",[],"/Users/ritterj/Development/crwn-clothing/src/componen